  "scripts": {
    "backend:up": "docker-compose build && docker-compose up -d",
    "backend:stop": "docker-compose kill",
    "backend:down": "docker-compose down",
    "build": "rm -rf dist && mkdir dist && tsc",
    "start": "TYPEORM_MODE=build node dist/src/server/index.js",
    "production": "TYPEORM_MODE=build pm2-runtime dist/src/server/index.js",
    "dev": "TYPEORM_MODE=dev nodemon src/server/index.ts",
    "test": "jest --runInBand",
    "typeorm": "./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js",
    "typeorm:generate-migration": "./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:generate -n ",
    "insert-test-data": "ts-node src/database/setup-mock-user.ts",
    "configure-database": "ts-node src/database/setup/setup-roles.ts",
    "seeds:development": "TYPEORM_MODE=dev ts-node src/database/seeds/development"
  },
